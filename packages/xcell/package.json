{
  "name": "xcell",
  "version": "0.0.7",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": "https://github.com/tomazy/xcell/tree/master/packages/xcell",
  "author": {
    "email": "tomazy@go2.pl",
    "name": "Tomek Maszkowski"
  },
  "description": "reactive cells for spreadsheet-like calculations",
  "keywords": [
    "spreadsheet",
    "reactive cells",
    "reactive programming",
    "directed acyclic graph",
    "DAG",
    "FRP"
  ],
  "devDependencies": {
    "@types/jasmine": "^2.8.4",
    "@types/jest": "^22.0.1",
    "@types/node": "^9.3.0",
    "jest": "^22.1.4",
    "npm-run-all": "^4.1.2",
    "ts-jest": "^22.0.1",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      ".(ts|tsx)": "../../node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/*.test.ts"
    ]
  },
  "scripts": {
    "clean": "rm -fr dist/* debug",
    "prebuild": "npm run clean",
    "build": "npm-run-all -p build:node build:umd",
    "build:node": "tsc",
    "build:umd": "webpack -p",
    "build:for-debug": "tsc -p ./tsconfig.debug.json --pretty",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  }
}
